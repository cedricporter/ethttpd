CPPFLAGS=-I../include

top=../
ede_FILES=Project.ede Makefile

ethttpd_SOURCES=main.c
ethttpd_OBJ= main.o
CC=gcc
C_COMPILE=$(CC) $(DEFS) $(INCLUDES) $(CPPFLAGS) $(CFLAGS)
C_DEPENDENCIES=-Wp,-MD,.deps/$(*F).P
C_LINK=$(CC) $(CFLAGS) $(LDFLAGS) -L.
VERSION=1.0
DISTDIR=$(top)ethttpd-project-$(VERSION)/src
top_builddir = ..

DEP_FILES=.deps/main.P

all: ethttpd

DEPS_MAGIC := $(shell mkdir .deps > /dev/null 2>&1 || :)
-include $(DEP_FILES)

.PHONY: check-syntax
check-syntax:
	gcc $(CPPFLAGS) -Wall -Wextra -pedantic -fsyntax-only $(CHK_SOURCES)

%.o: %.c
	@echo '$(C_COMPILE) -c $<'; \
	$(C_COMPILE) $(C_DEPENDENCIES) -o $@ -c $<

ethttpd: $(ethttpd_OBJ)
	$(C_LINK) -o $@ $^ $(LDDEPS)

tags:


clean:
	rm -f *.mod *.o *.obj .deps/*.P .lo

.PHONY: dist

dist:
	mkdir $(DISTDIR)
	cp $(ethttpd_SOURCES) $(ede_FILES) $(DISTDIR)

